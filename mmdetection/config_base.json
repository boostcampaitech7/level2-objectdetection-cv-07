                "Metal",
                "Glass",
                "Plastic",
                "Styrofoam",
                "Plastic bag",
                "Battery",
                "Clothing"
            ]
        }
    },
    "evaluation": {
        "interval": 1,
        "metric": "bbox"
    },
    "optimizer": {
        "type": "SGD",
        "lr": 0.02,
        "momentum": 0.9,
        "weight_decay": 0.0001
    },
    "optimizer_config": {
        "grad_clip": {
            "max_norm": 35,
            "norm_type": 2
        }
    },
    "lr_config": {
        "policy": "step",
        "warmup": "linear",
        "warmup_iters": 500,
        "warmup_ratio": 0.001,
        "step": [
            8,
            11
        ]
    },
    "runner": {
        "type": "EpochBasedRunner",
        "max_epochs": 12
    },
    "checkpoint_config": {
        "max_keep_ckpts": 3,
        "interval": 1
    },
    "log_config": {
        "interval": 50,
        "hooks": [
            {
                "type": "TextLoggerHook"
            }
        ]
    },
    "custom_hooks": [
        {
            "type": "NumClassCheckHook"
        }
    ],
    "dist_params": {
        "backend": "nccl"
    },
    "log_level": "INFO",
    "load_from": null,
    "resume_from": null,
    "workflow": [
        [
            "train",
            1
        ]
    ],
    "opencv_num_threads": 0,
    "mp_start_method": "fork",
    "auto_scale_lr": {
        "enable": false,
        "base_batch_size": 16
    },
    "seed": 2022,
    "gpu_ids": [
        0
    ],
    "work_dir": "./work_dirs/faster_rcnn_r50_fpn_1x_trash",
    "device": "cuda"
