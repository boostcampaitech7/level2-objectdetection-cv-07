        "test": {
            "type": "CocoDataset",
            "ann_file": "../../../dataset/test.json",
            "img_prefix": "../../../dataset/",
            "pipeline": [
                {
                    "type": "LoadImageFromFile"
                },
                {
                    "type": "MultiScaleFlipAug",
                    "img_scale": [
                        512,
                        512
                    ],
                    "flip": false,
                    "transforms": [
                        {
                            "type": "Resize",
                            "keep_ratio": true
                        },
                        {
                            "type": "RandomFlip"
                        },
                        {
                            "type": "Normalize",
                            "mean": [
                                123.675,
                                116.28,
                                103.53
                            ],
                            "std": [
                                58.395,
                                57.12,
                                57.375
                            ],
                            "to_rgb": true
                        },
                        {
                            "type": "Pad",
                            "size_divisor": 32
                        },
                        {
                            "type": "ImageToTensor",
                            "keys": [
                                "img"
                            ]
                        },
                        {
                            "type": "Collect",
                            "keys": [
                                "img"
                            ]
                        }
